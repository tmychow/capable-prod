{
    "crons": [
        {
            "path": "/cron/pickup-files",
            "schedule": "*/5 * * * *"
        },
        {
            "path": "/cron/sync-studies",
            "schedule": "*/5 * * * *"
        },
        {
            "path": "/cron/sync-peptides",
            "schedule": "0 0 * * *"
        }
    ],
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python@4.8.0"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": [
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "*",
                "Access-Control-Allow-Headers": "*",
                "Access-Control-Max-Age": "86400"
            },
            "status": 204
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}